{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "0.0.26",
  "default_locale": "en",
  "icons": {
    "48": "icon.png",
    "96": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "contextMenus",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png"
    },
    "default_popup": "options/options.html",
    "theme_icons": [
      {
        "light": "icon-dark-16.png",
        "dark": "icon-16.png",
        "size": 16
      },
      {
        "light": "icon-dark-32.png",
        "dark": "icon-32.png",
        "size": 32
      }
    ]
  }
}
