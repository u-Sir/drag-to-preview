{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "0.0.25",
  "default_locale": "en",
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg",
    "128": "icon.svg"
  },
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "cookies",
    "*://*/*"
  ],
  "browser_action": {
    "default_popup": "options/options.html"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "drag_to_privew@uxer"
    }
  }
}
