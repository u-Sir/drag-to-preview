{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "0.0.26",
  "default_locale": "en",
  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "cookies",
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png"
    },
    "default_popup": "options/options.html",
    "theme_icons": [
      {
        "light": "icon-dark-16.png",
        "dark": "icon-16.png",
        "size": 16
      },
      {
        "light": "icon-dark-32.png",
        "dark": "icon-32.png",
        "size": 32
      }
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "drag_to_privew@uxer"
    }
  }
}
